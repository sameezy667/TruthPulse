{
  "name": "sachai",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest --run",
    "test:watch": "vitest",
    "export": "next build && next export",
    "cap:build": "next build && next export && npx cap sync",
    "cap:sync": "npx cap sync",
    "cap:add:ios": "npx cap add ios",
    "cap:add:android": "npx cap add android",
    "cap:ios": "npx cap open ios",
    "cap:android": "npx cap open android",
    "genai:run": "ts-node scripts/genai_generate.ts",
    "config:dev": "node scripts/switch-config.js dev",
    "config:prod": "node scripts/switch-config.js prod",
    "build:apk:dev": "npm run config:dev && npx cap sync android && cd android && ./gradlew assembleDebug",
    "build:apk:prod": "npm run config:prod && npx cap sync android && cd android && ./gradlew assembleDebug"
  },
  "dependencies": {
    "@ai-sdk/google": "^3.0.1",
    "@ai-sdk/react": "^3.0.3",
    "@capacitor-mlkit/barcode-scanning": "^8.0.0",
    "@capacitor/android": "^7.2.0",
    "@capacitor/camera": "^7.0.0",
    "@capacitor/cli": "^7.2.0",
    "@capacitor/core": "^7.2.0",
    "@capacitor/haptics": "^7.0.0",
    "@capacitor/preferences": "^7.0.0",
    "@google/genai": "^1.34.0",
    "@ionic/pwa-elements": "^2.0.0",
    "ai": "^6.0.3",
    "clsx": "^2.1.1",
    "framer-motion": "^11.11.17",
    "geist": "^1.3.1",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.462.0",
    "mime": "^3.0.0",
    "next": "^15.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.4.0",
    "tesseract.js": "^7.0.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/node": "^20.19.27",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@vitest/ui": "^4.0.16",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.1.0",
    "fast-check": "^4.5.2",
    "jsdom": "^27.4.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.1",
    "typescript": "^5.7.2",
    "vitest": "^4.0.16"
  }
}
